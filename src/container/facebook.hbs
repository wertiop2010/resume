<div>
    <main>
        <section>
            <div>
                <h1>{{name}}</h1>
                <ul>
                    {{#each users}}
                       <span class="badge rounded-pill text-bg-danger">Users: </span> 
                        <li>
                            ID: {{id}}
                        </li>
                        <li>
                            name: {{name}}
                        </li>
                        <li>
                            gender: {{gender}}
                        </li>
                        <li>
                            age: {{age}}
                        </li>
                        <li>
                            <ul>
                            {{#each friends}}
                            <span class="badge rounded-pill text-bg-success">friends: </span>
                                <li>
                                ID: {{id}}
                                </li>
                                <li>
                                    name: {{name}}
                                </li>
                                <li>
                                    gender: {{gender}}
                                </li>
                                <li>
                                    age: {{age}}
                                </li>
                                <li>
                                    <ul>
                                    {{#each mutual_friends}}
                                    <span class="badge rounded-pill text-bg-primary">mutual_friends: </span> 
                                        <li>
                                        ID: {{id}}
                                        </li>
                                        <li>
                                            name: {{name}}
                                        </li>
                                        <li>
                                            gender: {{gender}}
                                        </li>
                                        <li>
                                            age: {{age}}
                                        </li>
                                    {{/each}}
                                    </ul>
                                </li>
                            {{/each}}
                            </ul>
                            <ul>
                            {{#each groups}}
                                <span class="badge rounded-pill text-bg-primary">groups: </span>
                                <li>
                                    ID: {{id}}
                                </li>
                                <li>
                                    name: {{name}}
                                </li>
                                <li>
                                    description: {{description}}
                                </li>
                                <li>
                                    <ul>
                                        {{#each members}}
                                        members:
                                        <li>
                                        ID: {{id}}
                                        </li>
                                        <li>
                                            name: {{name}}
                                        </li>
                                        <li>
                                            gender: {{gender}}
                                        </li>
                                        <li>
                                            age: {{age}}
                                        </li>
                                        {{/each}}
                                    </ul>
                                </li>
                            {{/each}}
                            </ul>
                        </li>
                    {{/each}}
                </ul>
            </div>
        </section>
    </main>
</div>

