<main>
    <section>
        <div class="row">
         
            {{#with navigation}}
                <button class="btn btn-link col-2" href="{{links.[0].href}}">{{links.[0].text}}</button>
                <button class="btn btn-link col-2" href="{{links.[1].href}}">{{links.[1].text}}</button>
                <button class="btn btn-link col-2" href="{{links.[2].href}}">{{links.[2].text}}</button>
                <button class="btn btn-primary col-2" href="{{button.href}}">{{button.text}}</button>
            {{/with}}
           
        </div>
    </section>

    <hr>
    <br>
    <section>
        <div>
        
            {{#with header}}
                <h1>{{title}}</h1>
                <p>{{description}}</p>
                {{#with button}}
                <button class="btn btn-primary" href="{{button.href}}">{{text}}</button>
                {{/with}}
            {{/with}}
           
        </div>
    </section>

    <hr>
    <br>
    <section>
        <h2>{{city.title}}</h2>
        <div class="row gy-4">      
            <div class="col-5">
                <div class="card">
                    <img class="card-img-top" alt="{{city.cards.[0].picture.alt}}" src="{{city.cards.[0].picture.src}}">
                    <div class="card-body">
                        <h6 class="card-title">{{city.cards.[0].title}}</h6>
                        <p><div class="">{{city.cards.[0].description}}</div></p>
                        <button class="btn btn-primary" href="{{city.cards.[0].button.href}}">{{city.cards.[0].button.text}}</button>
                    </div>
                </div>
            </div>
             <div class="col-5">
                <div class="card">
                    <img class="card-img-top" alt="{{city.cards.[1].picture.alt}}" src="{{city.cards.[0].picture.src}}">
                    <div class="card-body">
                        <h6 class="card-title">{{city.cards.[1].title}}</h6>
                        <p><div class="">{{city.cards.[1].description}}</div></p>
                        <button class="btn btn-primary" href="{{city.cards.[1].button.href}}">{{city.cards.[1].button.text}}</button>
                    </div>
                </div>
            </div>
             <div class="col-5">
                <div class="card">
                    <img class="card-img-top" alt="{{city.cards.[2].picture.alt}}" src="{{city.cards.[2].picture.src}}">
                    <div class="card-body">
                        <h6 class="card-title">{{city.cards.[2].title}}</h6>
                        <p><div class="">{{city.cards.[2].description}}</div></p>
                        <button class="btn btn-primary" href="{{city.cards.[2].button.href}}">{{city.cards.[2].button.text}}</button>
                    </div>
                </div>
            </div>
             <div class="col-5">
                <div class="card">
                    <img class="card-img-top" alt="{{city.cards.[3].picture.alt}}" src="{{city.cards.[3].picture.src}}">
                    <div class="card-body">
                        <h6 class="card-title">{{city.cards.[3].title}}</h6>
                        <p><div class="">{{city.cards.[3].description}}</div></p>
                        <button class="btn btn-primary" href="{{city.cards.[3].button.href}}">{{city.cards.[3].button.text}}</button>
                    </div>
                </div>
            </div>               
        </div>
    </section>

    <hr>
    <br>
    <section>
        <div>
            <h2>{{winner.title}}</h2>
             <table class="table">
                <thead>
                    <tr>
                        <th>
                            {{winner.table.head.[0]}}
                        </th>
                        <th>
                            {{winner.table.head.[1]}}
                        </th>
                        <th>
                            {{winner.table.head.[2]}}
                        </th>
                         <th>
                            {{winner.table.head.[3]}}
                        </th>
                    </tr>
                </thead>

                <body>
                    <tr>
                        <td>
                            {{winner.table.user.[0].place}}
                        </td>
                        <td>
                            {{winner.table.user.[0].name}}
                        </td>
                        <td>
                            {{winner.table.user.[0].surname}}
                        </td>
                        <td>
                            {{winner.table.user.[0].nickname}}
                        </td>
                    </tr>
                     <tr>
                        <td>
                            {{winner.table.user.[1].place}}
                        </td>
                        <td>
                            {{winner.table.user.[1].name}}
                        </td>
                        <td>
                            {{winner.table.user.[1].surname}}
                        </td>
                        <td>
                            {{winner.table.user.[1].nickname}}
                        </td>
                    </tr>
                     <tr>
                        <td>
                            {{winner.table.user.[2].place}}
                        </td>
                        <td>
                            {{winner.table.user.[2].name}}
                        </td>
                        <td>
                            {{winner.table.user.[2].surname}}
                        </td>
                        <td>
                            {{winner.table.user.[2].nickname}}
                        </td>
                    </tr>
                </body>
            </table>
        </div>
    </section>
    
    <hr>
    <br>
    <section>
        <div class="row gy-4"> 
    
            <h2>{{gallery.title}}</h2>
            <br>

            <div class="card col-4">
                <div class="card-header">
                    <b>{{winner.table.head.[1]}}</b>
                </div>
                    <ul class="list-group">
                        <li class="list-group-item ">{{winner.table.user.[0].place}}</li>
                        <li class="list-group-item ">{{winner.table.user.[0].name}}</li>  
                        <li class="list-group-item ">{{winner.table.user.[0].surname}}</li>
                        <li class="list-group-item ">{{winner.table.user.[0].nickname}}</li>  
                    </ul>
            </div>

            <div class="card col-4">
                <div class="card-header">
                    <b>{{winner.table.head.[2]}}</b>
                </div>
                    <ul class="list-group">
                        <li class="list-group-item ">{{winner.table.user.[1].place}}</li>
                        <li class="list-group-item ">{{winner.table.user.[1].name}}</li>  
                        <li class="list-group-item ">{{winner.table.user.[1].surname}}</li>
                        <li class="list-group-item ">{{winner.table.user.[1].nickname}}</li>  
