                        <li class="list-group-item ">{{winner.table.user.[2].name}}</li>  
                        <li class="list-group-item ">{{winner.table.user.[2].surname}}</li>
                        <li class="list-group-item ">{{winner.table.user.[2].nickname}}</li>  
                    </ul>
            </div>
        </div>
    </section>

    <hr>
    <br>
    <section>
        <div class="row gy-5">
            <h2>{{relaxion.title}}</h2>   
            <div class="col-5">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{relaxion.cards.[0].header}}</div>
                    <div class="card-body">
                        <h6 class="card-title">{{relaxion.cards.[0].title}}</h6>
                        <p><div class="">{{relaxion.cards.[0].description}}</div></p>
                    </div>
                </div>
            </div>

            <div class="col-5">
                <div class="card text-bg-secondary">
                    <div class="card-header">{{relaxion.cards.[1].header}}</div>
                    <div class="card-body">
                        <h6 class="card-title">{{relaxion.cards.[1].title}}</h6>
                        <p><div class="">{{relaxion.cards.[1].description}}</div></p>
                        <br>
                    </div>
                </div>
            </div>

            <div class="col-5">
