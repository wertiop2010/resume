<div>
     {{#with main}}
        <main>
            <section>
                {{> title big=true data="My educations" big=true}}
                <ul>
                    {{#each educations}}
                        <li>
                            {{name}}
                            {{#unless isEnd}}
                            (Ще в процесі) 
                            {{/unless}}
                        </li>
                    {{/each}}
                </ul>
            </section>
                <br />
            <section>
                {{> title big=false data="My certificates"}}
                    <ul>
                        {{#each certificates}}
                            <li>
                                {{name}}
                                ({{id}})
                            </li>
                        {{/each}}
                    </ul>
            </section>
        </main>
    {{/with}}
</div>