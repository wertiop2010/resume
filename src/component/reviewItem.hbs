 <section>
            {{#each reviewList}}
            <div class="card p-4 my-4">
                <div class="card">
                    <div class="card-body">
                        {{#with title}}
                        <h2 class="card-title">{{userName}}</h2>
                        <p><div class="">{{caption}}</div></p>
                        <p><div class="">{{date}}</div></p>
                    {{/with}}    
                    </div>
                </div>
                {{#with reviewBody}}
                {{#with seller}}
                <div class="card">
                    <div class="card-body">
                        <p><div class="">{{title}}: {{value}}</div></p>
                        {{/with}}
                        {{#with rating}}
                        <p><div class="">{{title}}: {{value}}</div></p>
                        {{/with}}

                        {{#each text}}
                        <p>{{this}}</p>

                        {{/each}}
                        {{#with advantages}}
                        <p><div class=""><b>{{title}}:</b></div></p>
                        <p><div class="">{{text}}</div></p>
                        {{/with}}
                        {{#with disadvantages}}
                        <p><div class=""><b>{{title}}:</b></div></p>
                        <p><div class="">{{text}}</div></p>
                        {{/with}}
                        {{#each images}}
                        <img  alt="image" src="{{this}}">
                        {{/each}}
                    </div>
                </div>
                {{#each coments}}
                <div class="card m-2">
                     <h4 class="card-title m-2">{{userName}}</h4>
                     <div class="card-body">
                        <p>{{date}}</p>
                        <p>{{text}}</p></div>
                    </div> 
                {{/each}}
                {{/with}} 
            </div>
        {{/each}}
        </section>