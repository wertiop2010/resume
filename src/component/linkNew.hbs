<section> 
    {{#with navigation}}
        <div class="card  p-3">
            <div class="nav">
            {{#each links}} 
                <div class="nav-item ">   
                    <a href="{{href}}" class="nav-link {{#unless href}}disabled{{/unless}}">{{text}}</a>
                </div>
            {{/each}}
            </div>
        </div>
    {{/with}}
</section>